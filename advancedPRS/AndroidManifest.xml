<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.advancedprs"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.advancedprs.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.advancedprs.ViewQuestion"
            android:label="@string/title_activity_view_question" >
        </activity>
        <activity
            android:name="com.example.advancedprs.MultChoice"
            android:label="@string/title_activity_mult_choice" >
        </activity>
        <activity
            android:name="com.example.advancedprs.ShortEssay"
            android:label="@string/title_activity_short_essay" >
        </activity>
        <activity
            android:name="com.example.advancedprs.Drawable"
            android:label="@string/title_activity_drawable" >
        </activity>
    </application>

</manifest>
